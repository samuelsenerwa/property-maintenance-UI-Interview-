{
  "expo": {
    "name": "property-maintenance",
    "slug": "property-maintenance",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "propertymaintenance",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ynh.propertymaintenance"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ynh.propertymaintenance",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash-icon.png"
        }
      ],
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "enablePngCrunchInReleaseBuilds": true,
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true,
            "useAndroidX": true
          }
        }
      ],
      "react-native-edge-to-edge",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share the plumber"
        }
      ],
      "expo-build-properties"
    ],
    "extra": {
      "eas": {
        "projectId": "90d0e98d-3cde-4130-a569-a830d991c5da"
      }
    }
  }
}